{
    "version": 6,
    "cmakeMinimumRequired": {
        "major": 3,
        "minor": 25,
        "patch": 0
    },
    "include": [
        "../cmake/CMakePresets.json"
    ],
    "configurePresets": [
        {
            "name": "ci-examples",
            "displayName": "Build Examples",
            "inherits": "release",
            "cacheVariables": {
                "JWT_BUILD_EXAMPLES": "ON"
            }
        }
    ],
    "buildPresets": [
        {
            "name": "ci-examples",
            "configurePreset": "ci-examples",
            "configuration": "Release",
            "targets": [
                "rsa-create",
                "rsa-verify"
            ]
        }
    ]
}